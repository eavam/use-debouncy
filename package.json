{
  "name": "use-debouncy",
  "version": "2.1.7",
  "author": "Egor Avakumov",
  "description": "ðŸŒ€Small (120 bytes) debounce effect hook for React with TypeScript support",
  "license": "MIT",
  "keywords": [
    "debounce",
    "use-debounce",
    "react debounce",
    "debounce hook",
    "use-debouncy",
    "use-effect",
    "react",
    "react hook",
    "hooks",
    "typescript"
  ],
  "repository": {
    "url": "https://github.com/eavam/use-debouncy.git",
    "type": "git"
  },
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "module": "lib/index.es.js",
  "sideEffects": false,
  "scripts": {
    "size": "size-limit",
    "test": "jest",
    "test:unit": "jest --config=jest.unit.config.js",
    "test:e2e": "jest --config=jest.e2e.config.js",
    "build": "rollup -c",
    "prebuild": "rimraf lib compile && tsc --outDir compile --declarationDir lib --declaration true"
  },
  "peerDependencies": {
    "react": "^16.8.0"
  },
  "dependencies": {},
  "devDependencies": {
    "@commitlint/cli": "9.1.1",
    "@commitlint/config-conventional": "9.1.1",
    "@rollup/plugin-commonjs": "14.0.0",
    "@rollup/plugin-typescript": "5.0.2",
    "@semantic-release/changelog": "5.0.1",
    "@semantic-release/commit-analyzer": "8.0.1",
    "@semantic-release/exec": "5.0.0",
    "@semantic-release/git": "9.0.0",
    "@semantic-release/github": "7.0.7",
    "@semantic-release/npm": "7.0.5",
    "@semantic-release/release-notes-generator": "9.0.1",
    "@size-limit/preset-small-lib": "4.5.6",
    "@size-limit/time": "4.5.6",
    "@testing-library/react-hooks": "3.4.1",
    "@types/jest": "26.0.9",
    "@types/react": "16.9.43",
    "@types/react-dom": "16.9.8",
    "@typescript-eslint/eslint-plugin": "3.9.0",
    "@typescript-eslint/parser": "3.9.0",
    "bit-bin": "14.8.8",
    "eslint": "7.6.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-react": "7.20.5",
    "eslint-plugin-react-hooks": "4.0.8",
    "eslint-plugin-unicorn": "21.0.0",
    "husky": "4.2.5",
    "jest": "26.3.0",
    "jest-playwright-preset": "1.3.1",
    "parcel": "1.12.4",
    "playwright": "1.3.0",
    "prettier": "2.0.5",
    "pretty-quick": "2.0.1",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-test-renderer": "16.13.1",
    "rimraf": "3.0.2",
    "rollup": "2.23.1",
    "rollup-plugin-terser": "6.1.0",
    "semantic-release": "17.1.1",
    "size-limit": "4.5.6",
    "ts-jest": "26.2.0",
    "tslib": "2.0.1",
    "typescript": "3.9.7"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "size-limit": [
    {
      "name": "use-debouncy",
      "limit": "120 B",
      "path": "lib/index.js",
      "import": "{ useDebouncy }",
      "running": false
    },
    {
      "name": "use-debouncy ES6",
      "limit": "120 B",
      "path": "lib/index.es.js",
      "import": "{ useDebouncy }",
      "running": false
    }
  ],
  "files": [
    "lib/"
  ],
  "publishConfig": {
    "access": "public"
  },
  "bit": {
    "env": {
      "compiler": "bit.envs/compilers/react-typescript@3.1.54"
    },
    "componentsDefaultDirectory": "components/{name}",
    "packageManager": "yarn"
  }
}
