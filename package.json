{
  "name": "use-debouncy",
  "version": "1.9.3",
  "author": "Egor Avakumov",
  "description": "ðŸŒ€Tiny (<150 bytes) debounce react effect hook with typescript support",
  "license": "MIT",
  "keywords": [
    "debounce",
    "use-debounce",
    "react debounce",
    "debounce hook",
    "use-debouncy",
    "use-effect",
    "react",
    "react hook",
    "hooks",
    "typescript"
  ],
  "repository": {
    "url": "https://github.com/eavam/use-debouncy.git",
    "type": "git"
  },
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "module": "lib/index.es.js",
  "scripts": {
    "size": "size-limit",
    "test": "jest",
    "build": "rollup -c",
    "prebuild": "rimraf lib compile && tsc --outDir compile --declarationDir lib --declaration true"
  },
  "peerDependencies": {
    "react": "^16.8.0"
  },
  "dependencies": {},
  "devDependencies": {
    "@commitlint/cli": "9.0.1",
    "@commitlint/config-conventional": "9.0.1",
    "@rollup/plugin-commonjs": "13.0.0",
    "@rollup/plugin-typescript": "5.0.1",
    "@semantic-release/changelog": "5.0.1",
    "@semantic-release/commit-analyzer": "8.0.1",
    "@semantic-release/git": "9.0.0",
    "@semantic-release/github": "7.0.7",
    "@semantic-release/npm": "7.0.5",
    "@semantic-release/release-notes-generator": "9.0.1",
    "@size-limit/preset-small-lib": "4.5.2",
    "@size-limit/time": "4.5.2",
    "@testing-library/react-hooks": "3.3.0",
    "@types/jest": "26.0.3",
    "@types/react": "16.9.38",
    "@typescript-eslint/eslint-plugin": "3.5.0",
    "@typescript-eslint/parser": "3.5.0",
    "eslint": "7.3.1",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-react": "7.20.3",
    "eslint-plugin-react-hooks": "4.0.5",
    "eslint-plugin-unicorn": "20.1.0",
    "husky": "4.2.5",
    "jest": "26.1.0",
    "prettier": "2.0.5",
    "pretty-quick": "2.0.1",
    "react": "16.13.1",
    "react-test-renderer": "16.13.1",
    "rimraf": "3.0.2",
    "rollup": "2.18.2",
    "rollup-plugin-terser": "6.1.0",
    "semantic-release": "17.1.1",
    "size-limit": "4.5.2",
    "ts-jest": "26.1.1",
    "tslib": "2.0.0",
    "typescript": "3.9.6"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "size-limit": [
    {
      "limit": "150 B",
      "path": [
        "lib/index.js"
      ],
      "import": "{ createStore }",
      "running": true,
      "gzip": true,
      "brotli": false
    },
    {
      "limit": "150 B",
      "path": [
        "lib/index.es.js"
      ],
      "import": "{ createStore }",
      "running": true,
      "gzip": true,
      "brotli": false
    }
  ],
  "files": [
    "lib/"
  ],
  "publishConfig": {
    "access": "public"
  }
}
