{
  "name": "use-debouncy",
  "version": "3.1.0",
  "author": "Egor Avakumov",
  "description": "ðŸŒ€ Small (180 bytes) debounce effect hook for React with TypeScript support",
  "license": "MIT",
  "keywords": [
    "debounce",
    "use-debounce",
    "react debounce",
    "debounce hook",
    "use-debouncy",
    "use-effect",
    "react",
    "react hook",
    "hooks",
    "typescript"
  ],
  "repository": {
    "url": "https://github.com/eavam/use-debouncy.git",
    "type": "git"
  },
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "module": "lib/index.es.js",
  "sideEffects": false,
  "scripts": {
    "size": "size-limit",
    "test": "jest",
    "serve:e2e": "npx parcel cypress/app/index.html",
    "cypress:open": "cypress open",
    "lint": "eslint . --ext .js,.ts",
    "build": "rollup -c",
    "clear": "rimraf lib",
    "prebuild": "npm run clear && tsc -d --emitDeclarationOnly --declarationDir lib",
    "semantic-release": "semantic-release",
    "postinstall": "is-ci || husky install"
  },
  "peerDependencies": {
    "react": "^16.8.0 || ^17.0.0"
  },
  "dependencies": {},
  "devDependencies": {
    "@commitlint/cli": "11.0.0",
    "@commitlint/config-conventional": "11.0.0",
    "@rollup/plugin-commonjs": "17.1.0",
    "@rollup/plugin-typescript": "8.1.1",
    "@semantic-release/changelog": "5.0.1",
    "@semantic-release/commit-analyzer": "8.0.1",
    "@semantic-release/exec": "5.0.0",
    "@semantic-release/git": "9.0.0",
    "@semantic-release/github": "7.2.0",
    "@semantic-release/npm": "7.0.10",
    "@semantic-release/release-notes-generator": "9.0.1",
    "@size-limit/preset-small-lib": "4.9.2",
    "@size-limit/time": "4.9.2",
    "@testing-library/react-hooks": "5.0.3",
    "@types/jest": "26.0.20",
    "@types/react": "17.0.2",
    "@types/react-dom": "17.0.1",
    "@typescript-eslint/eslint-plugin": "4.15.0",
    "@typescript-eslint/parser": "4.15.0",
    "bit-bin": "14.8.8",
    "cypress": "6.4.0",
    "eslint": "7.20.0",
    "eslint-config-prettier": "7.2.0",
    "eslint-plugin-cypress": "2.11.2",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-prettier": "3.3.1",
    "eslint-plugin-react": "7.22.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "eslint-plugin-unicorn": "28.0.0",
    "husky": "5.0.9",
    "is-ci": "2.0.0",
    "jest": "26.6.3",
    "prettier": "2.2.1",
    "pretty-quick": "3.1.0",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-test-renderer": "17.0.1",
    "rimraf": "3.0.2",
    "rollup": "2.39.0",
    "rollup-plugin-terser": "7.0.2",
    "semantic-release": "17.3.8",
    "size-limit": "4.9.2",
    "ts-jest": "26.5.1",
    "tslib": "2.1.0",
    "typescript": "4.1.5"
  },
  "size-limit": [
    {
      "name": "use-debouncy",
      "limit": "180 B",
      "path": "lib/index.js",
      "import": "{ useDebouncy }",
      "running": false
    },
    {
      "name": "use-debouncy ES6",
      "limit": "180 B",
      "path": "lib/index.es.js",
      "import": "{ useDebouncy }",
      "running": false
    }
  ],
  "files": [
    "lib/"
  ],
  "publishConfig": {
    "access": "public"
  },
  "bit": {
    "env": {
      "compiler": "bit.envs/compilers/react-typescript@3.1.54"
    },
    "componentsDefaultDirectory": "components/{name}",
    "packageManager": "npm"
  }
}
